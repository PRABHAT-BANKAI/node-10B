<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>dashboard</h1>
    <form action="/insertData" method="post" enctype="multipart/form-data">
      <table border="">
        <tr>
          <td>name</td>
          <td>
            <input
              type="text"
              name="studentName"
              placeholder="enter your name"
            />
          </td>
        </tr>
        <tr>
          <td>email</td>
          <td>
            <input type="email" placeholder="enter your email" name="email" />
          </td>
        </tr>
        <tr>
          <td>image</td>
          <td><input type="file" name="image" /></td>
        </tr>
        <tr>
          <td></td>
          <td><input type="submit" value="submit" /></td>
        </tr>
      </table>
    </form>

    <table border="">
      <tr>
        <td>name</td>
        <td>email</td>
        <td>image</td>
        <td>action</td>
      </tr>
      <% studentData.map((student)=>{ %>
      <tr>
        <td><%= student.studentName %></td>
        <td><%= student.email %></td>
        <td><img src="<%= student.image %>" alt="image" height="100" /></td>
        <td>
          <a href="/deleteData/<%= student.id %>"><button>delete</button></a>
          <a href="/updateData/<%= student.id %>"><button>update</button></a>
        </td>
      </tr>

      <% }) %>
    </table>
  </body>
</html>
